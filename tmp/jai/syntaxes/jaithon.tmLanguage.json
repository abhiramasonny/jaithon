{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "jaithon",
	"patterns": [
	  {
		"include": "#keywords"
	  },
	  {
		"include": "#strings"
	  }
	],
	"repository": {
	  "keywords": {
		"patterns": [
		  {
			"name": "keyword.control.jaithon",
			"match": "\\b(if|while|for|return|var|array|print)\\b"
		  }
		]
	  },
	  "strings": {
		"name": "string.quoted.double.jaithon",
		"begin": "\"",
		"end": "\"",
		"patterns": [
		  {
			"name": "constant.character.escape.jaithon",
			"match": "\\\\."
		  }
		]
	  }
	},
	"scopeName": "source.jai"
  }
  